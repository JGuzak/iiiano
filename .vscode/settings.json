{
	"Lua.diagnostics.globals": [
		"grid_size_x",
		"grid_size_y",
		"grid_led",
		"grid_refresh",
		"grid_led_all",
		"midi_note_off",
		"midi_note_on",
		"linlin",
		"round",
		"metro",
		"get_time",
		"clamp",
		"grid_led_get",
		"midi_cc"
	],
	"Lua.diagnostics.disable": [
		"lowercase-global"
	],
	"editor.inlineSuggest.enabled": false,
	"Lua.codeLens.enable": true,
	"Lua.completion.callSnippet": "Both",
	"Lua.completion.displayContext": 1,
	"Lua.format.defaultConfig": {
		"space_around_table_field_list": "false",
		"space_before_attribute": "false",
		"space_before_function_open_parenthesis": "false",
		"space_before_function_call_open_parenthesis": "false",
		"space_before_closure_open_parenthesis": "false",
		"space_before_function_call_single_arg": "false",
		"space_before_open_square_bracket": "false",
		"space_inside_function_param_list_parentheses": "false",
		"space_inside_square_brackets": "false",
		"space_around_table_append_operator": "false",
		"ignore_spaces_inside_function_call": "false",
		"space_before_inline_comment": "0",
		"space_after_comment_dash": "false",
		"space_around_math_operator": "false",
		"space_after_comma": "false",
		"space_after_comma_in_for_statement": "false",
		"space_around_logical_operator": "false",
		"space_around_assign_operator": "none",
		"space_around_concat_operator": "none",
		"align_call_args": "false",
		"align_function_params": "true",
		"align_continuous_assign_statement": "true",
		"align_continuous_rect_table_field": "true",
		"align_continuous_line_space": "1",
		"align_if_branch": "true",
		"align_array_table": "false",
		"align_continuous_similar_call_args": "false",
		"align_continuous_inline_comment": "false",
		"align_chain_expr": "none",
		"never_indent_before_if_condition": "false",
